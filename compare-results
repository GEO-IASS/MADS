#!/bin/tcsh -f
if( ! -e $1 ) then
	echo '>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>'
	echo ERROR Verification failed!
	echo ERROR File $1 is missing!
	echo '>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>'
	exit 0
endif
if( ! -e $2 ) then
	echo '>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>'
	echo ERROR Verification failed!
	echo ERROR File $2 is missing!
	echo '>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>'
	exit 0
endif
grep -v Execution $1 | grep -v second >! $1_e_$$
grep -v Execution $2 | grep -v second >! $2_e_$$
# keeep this lines together
sdiff $1_e_$$ $2_e_$$ > $1_$$
set a = $?
# keeep this lines together
\rm -f $1_e_$$ $2_e_$$
if( $a != 0 ) then
	echo '>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>'
	echo ERROR Verification failed!
	tail $1_$$
	\rm -f $1_$$
	echo '>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>'
	exit 0
else
	echo Verification is successful!
	\rm -f $1_$$
	exit 0
endif
